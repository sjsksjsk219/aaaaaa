-- Doors 专用 ESP 配置
-- =============================================
local MonumentESP = AdvancedESP.new({"MonumentEntity", "柱子", "Monster"})
MonumentESP:SetConfig({
    ScanRecursive = true,
    MaxDistance = 500,
    HighlightColor = Color3.fromRGB(255, 182, 193),
    LabelSize = 12,
    ShowDistance = true,
    DisplayNames = {
        MonumentEntity = "柱子怪",
        Monster = "怪物"
    }
})

local SnareESP = AdvancedESP.new({"Snare", "地刺", "Trap"})
SnareESP:SetConfig({
    ScanRecursive = true,
    MaxDistance = 300,
    HighlightColor = Color3.fromRGB(144, 238, 144),
    LabelSize = 12,
    DisplayNames = {
        Snare = "地刺陷阱",
        Trap = "陷阱"
    }
})

local MandrakeESP = AdvancedESP.new({"Mandrake", "萝卜"})
MandrakeESP:SetConfig({
    ScanRecursive = true,
    MaxDistance = 200,
    HighlightColor = Color3.fromRGB(255, 215, 0),
    CheckMarker = true,
    MarkerName = "Body",
    LabelSize = 12,
    DisplayNames = {
        Mandrake = "毒萝卜"
    }
})

local DoorESP = AdvancedESP.new({"Door", "门", "Exit"})
DoorESP:SetConfig({
    ScanRecursive = true,
    MaxDistance = 1000,
    HighlightColor = Color3.fromRGB(100, 149, 237),
    LabelSize = 14,
    DisplayNames = {
        Door = "出口门",
        Exit = "出口"
    }
})

local GroundskeeperESP = AdvancedESP.new({"Groundskeeper", "监管", "Keeper"})
GroundskeeperESP:SetConfig({
    ScanRecursive = true,
    MaxDistance = 400,
    HighlightColor = Color3.fromRGB(220, 20, 60),
    LabelSize = 14,
    DisplayNames = {
        Groundskeeper = "监管者",
        Keeper = "守卫"
    }
})

-- =============================================
-- ESP 界面绑定
-- =============================================
local ESPTab = Window:CreateTab("透视功能", 4483362458) -- 使用图标ID

local function createESPToggle(name, esp)
    return ESPTab:CreateToggle({
        Name = name,
        CurrentValue = false,
        Callback = function(Value)
            if Value then 
                esp:Start() 
                Rayfield:Notify({
                    Title = "ESP 已开启",
                    Content = name.."透视激活",
                    Duration = 2
                })
            else 
                esp:Stop()
            end
        end
    })
end

createESPToggle("柱子透视", MonumentESP)
createESPToggle("监管透视", GroundskeeperESP)
createESPToggle("门透视", DoorESP)
createESPToggle("地刺透视", SnareESP)
createESPToggle("萝卜透视", MandrakeESP)

