local Rayfield = loadstring(game:HttpGet('https://sirius.menu/rayfield'))()

local Window = Rayfield:CreateWindow({
    Name = "被遗弃",
    LoadingTitle = "XB脚本--被遗弃",
    LoadingSubtitle = "by sjsksjsk",
    ConfigurationSaving = {
        Enabled = false,
        FolderName = nil,
        FileName = "脚本"
    },
    Discord = {
        Enabled = false,
        Invite = "discord.gg/Mspaint",
        RememberJoins = true
    },
    KeySystem = false,
    KeySettings = {
        Title ="卡密系统",
        Subtitle = "点击获取链接",
        Note = "无",
        FileName = "Link",
        SaveKey = true,
        GrabKeyFromSite = true,
        Key = {"https://pastebin.com/raw/5a5zfWn7"}
    }
})


Rayfield:Notify({
    Title = "欢迎使用本脚本",
    Content = "开的时候演都不要演",
    Duration = 2,
    Image = ☠️,
    Actions = {
        Ignore = {
            Name = "好的",
            Callback = function()
                print("用户点了好的")
            end
        },
    },
})


local MainTab = Window:CreateTab("主要",nil)

local Toggle = MainTab:CreateToggle({
    Name = "防挂机被踢",
    Callback = function(Value)
        if Value then
            Rayfield:Notify({
                Title = "防挂机被踢",
                Content = "已开启防挂机!",
                Duration = 2,
                Image = 😃
            })
            while Value do
                game:GetService("Players").LocalPlayer.PlayerData.Settings.Game.AFK.Value = false
                task.wait()
            end
        else
            Rayfield:Notify({
                Title = "防挂机被踢",
                Content = "已关闭防挂机!",
                Duration = 2,
                Image = 😱
            })
        end
    end,
})

local Toggle = MainTab:CreateToggle({
    Name = "开启Debug模式",
    Callback = function(Value)
        if Value then
            game:GetService("Players").LocalPlayer.PlayerData.Settings.Advanced.DebugMode.Value = true
            Rayfield:Notify({
                Title = "Debug",
                Content = "已开启Debug模式!",
                Duration = 2,
                Image = 😈
            })
        else
            game:GetService("Players").LocalPlayer.PlayerData.Settings.Advanced.DebugMode.Value = false
            Rayfield:Notify({
                Title = "Debug",
                Content = "已关闭Debug模式!",
                Duration = 2,
                Image = 👿
            })
        end
    end,
})

