local Toggle = MainTab:CreateToggle({
    Name = "æ— é™ä½“åŠ›",
    Callback = function(Value)
        if Value then
            Rayfield:Notify({
                Title = "æ— é™ä½“åŠ›",
                Content = "æ— é™ä½“åŠ›å·²å¼€å¯!",
                Duration = 2,
                Image = nil
            })
            
            --workspace.Players.Survivors.Noob.SpeedMultipliers.Sprinting
            
            local Survivors = game.Workspace.Players.Survivors
            for _, Player in pairs(Survivors:GetChildren()) do
                if Player.SpeedMultipliers.Sprinting then  --æ£€æµ‹æ˜¯å¦ä¸ºè‡ªå·±,åªæœ‰è‡ªå·±çš„æ‰ä¼šæœ‰Sprinting,ä¿®æ”¹ä¸äº†åˆ«äººçš„
                    while Value do
                        Player.SpeedMultipliers.Sprinting.Value = 3
                        task.wait()
                    end
                end
            end
        else
            Rayfield:Notify({
                Title = "æ— é™ä½“åŠ›",
                Content = "æ— é™ä½“åŠ›å·²å…³é—­!",
