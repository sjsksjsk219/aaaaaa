    Callback = function(Value)
        if Value then
           _G.WaitTime = tonumber(Value)
        else
            Rayfield:Notify({
                Title = "ä¿®æœº",
                Content = "è¯·è¾“å…¥å»¶è¿Ÿ!",
                Duration = 2,
                Image = ğŸ˜¡
            })
        end
    end,
})

