if game.PlaceId == 2413927524 then
  
local Rayfield = loadstring(game:HttpGet('https://sirius.menu/rayfield'))()
  local Window = Rayfield:CreateWindow({
    Name = "The Rake Script",
    LoadingTitle = "The Rake脚本",
    LoadingSubtitle = "by sjsksjsk",
    ConfigurationSaving = {
      Enabled = false,
      FoldrName = nil,
      FileName = "脚本"
      },
    Discord = {
      Enabled = false,
      Invite = "discord.gg/Mspaint",
      RememberJoins = true
      },
    KeySystem = true,
    KeySettings = {
      Title ="The Rake脚本卡密系统",
      Subtitle = "点击获取链接",
      Note = "无",
      FileName = "The Rake Link",
      SaveKey = false,
      GrabKeyFromSite = true,
      Key = {"https://pastebin.com/raw/kVe5WMyJ"}
      }
    })
local MainTab = Window:CreateTab("通用功能",nil)
local MainSection = MainTab:CreateSection("部分服务器不可用")

Rayfield:Notify({
  Title ="欢迎使用本脚本",
  Content = "开源",
  Duration = 2,
  Image = nil,
  Actions = {
    Ignore = {
      Name = "好的",
      Callback = function()
        print("用户点了好的")
        end
      },
    },
  })

local Button = MainTab:CreateButton({
  Name = "无限跳跃",
  Callback = function()
    _G.infinjump = not _G.infinjump
    if _G.infinJumpStarted == nil then
      _G.infinJumpStarted = true
      
      game.StarterGui:SetCore("SendNotification",{Title="The Rake Script";Text="无限跳跃已开启！";Duration=5;})
      
      local plr = game:GetService('Players').LocalPlayer
      local m = plr:GetMouse()
      m.KeyDown:connect(function(k)
        if _G.infinjump then
          if k:byte() == 32 then
            humanoid = game:GetService'Players'.LocalPlayer.Character:FindFirstChild0fClass('Humanoid')
            humanoid:ChangeState('Jumping')
            wait()
            humanoid:ChangeState('Seated')
            end
          end
        end)
      end
    end,
  })

local Slider = MainTab:CreateSlider({
  Name = "速度调节",
  Range = {0, 1000},
  Increment = 1,
  Suffix = "Speed",
  CurrentValue = 16,
  Flag = "Slider1",
  Callback = function(Value)
    game.Players.LocalPlayer.Character.Humanoid.WalkSpeed = (Value)
    end,
  })

local Dropdown = MainTab:CreateDropdown({
  Name = "穿墙",
  Options = {"关闭穿墙","开启穿墙"},
  CurrentOption = {"开启穿墙"},
  MultipleOptions = false,
  Flag = "TP",
  Callback = function(Option)
    game:GetService("RunService").Stepped:Connect(function()
      for _, part in pairs(game.Players.LocalPlayer.Character:GetDescendants()) do
        if part:IsA("BasePart") then
          part.CanCollide  = true
          end
          end
        end
   )end,
})

local Button = MainTab:CreateButton({
  Name = "开启Dex",
  Callback = function()
    loadstring(game:HttpGet('https://raw.githubusercontent.com/infyiff/backup/main/dex.lua'))()
    end,
  })
local Button = MainTab:CreateButton({
  Name = "IY指令",
  Callback = function()
    loadstring(game:HttpGet('https://raw.githubusercontent.com/EdgeIY/infiniteyield/master/source'))()
    end,
  })
end
